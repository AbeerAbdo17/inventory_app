	<queryString>
		<![CDATA[SELECT
     sales.`DAT` AS sales_DAT,
     sales.`TOTAL` AS sales_TOTAL,
     sales.`PAID` AS sales_PAID,
     sales.`DEBIT` AS sales_DEBIT,
     sales.`USR` AS sales_USR,
     sales.`SHIFT` AS sales_SHIFT,
     sales.`CARD` AS sales_CARD,
     sales.`MON` AS sales_MON,
     sales.`id` AS sales_id,
     saledets.`FK` AS saledets_FK,
     saledets.`QTY` AS saledets_QTY,
     saledets.`BALANCE` AS saledets_BALANCE,
     saledets.`BATCH` AS saledets_BATCH,
     saledets.`UC` AS saledets_UC,
     saledets.`TC` AS saledets_TC,
     saledets.`item_name` AS saledets_item_name,
     saledets.`UP` AS saledets_UP,
saledets.`id` AS saledets_id,
     saledets.`TP` AS saledets_TP,
(select vat from company ) as vat1,
(select tel3 from company limit 1) as tax,
(select item_scname from items where items.item_id = saledets.id limit 1) as scname,
(select item_units from items where items.item_id = saledets.id limit 1) as unit,
     saledets.`ID` AS saledets_ID,
     barcode.`item_id` AS barcode_item_id,
     barcode.`code` AS barcode_code,
     items.`item_id` AS item_item_id,
     items.`item_units` AS item_packs,
     company.`name` AS company_name,
     company.`address` AS company_address,
     company.`id` AS company_id

FROM
     `sales` sales INNER JOIN `saledets`saledets  ON sales.`id` = saledets.`FK` inner join barcode on barcode.item_id = saledets.id inner join items on items.item_id = saledets.id ,
     `company` company
WHERE
     sales.id = $P{invoice_no}
group by saledets.item_name]]>
	</queryString>
