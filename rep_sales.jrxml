	<queryString>
		<![CDATA[SELECT
saledets.`fk` AS saledets_fk,
     saledets.`QTY` AS saledets_QTY,
     saledets.`GS` AS saledets_GS,

(select sum(paid) from sales where gs between $P{fdate} and $P{ldate}) as curcash,
     saledets.`PROFIT` AS saledets_PROFIT,
     saledets.`item_name` AS saledets_item_name,
    (select item_scname from items where items.item_id = saledets.id limit 1) as scname,
     saledets.`UC` AS saledets_UC,sum( saledets.`QTY`),sum(  saledets.`PROFIT`),sum( saledets.`tp`),
company.`name` AS company_name,
     company.`address` AS company_address,
     company.`id` AS company_id
FROM
     `saledets` saledets ,`company` company where saledets.`GS` >=$P{fdate} and saledets.`GS` <=$P{ldate} and refund = 0 group by saledets.`item_name`]]>
	</queryString>
